{
    "name": "terraform",
    "version": "0.1.0",
    "description": "A terraform example creating a VM on Azure",
    "keywords": [
        "terraform",
        "azure"
    ],
    "maintainers": [
        {
            "name": "Deis Labs",
            "email": "mabutch@microsoft.com",
            "url": "https://deislabs.io/"
        }
    ],
    "components": {
        "cnab": {
            "name": "cnab",
            "builder": "docker",
            "configuration": {
                "registry": "cnab"
            }
        }
    },
    "parameters": {
        "backend": {
            "type": "boolean",
            "defaultValue": false
        }
    },
    "credentials": {
        "tenant_id": {
            "env": "TF_VAR_tenant_id"
        },
        "client_id": {
            "env": "TF_VAR_client_id"
        },
        "client_secret": {
            "env": "TF_VAR_client_secret"
        },
        "subscription_id": {
            "env": "TF_VAR_subscription_id"
        },
        "ssh_authorized_key": {
            "env": "TF_VAR_ssh_authorized_key"
        }
    }
}
