{
    "name": "wordpress-mysql-bundle",
    "version": "0.1.0",
    "invocationImages": [
      {
        "imageType": "docker",
        "image": "michellenoorali/wordpress-mysql-bundle:0.1.0"
      }
    ],
    "images": null,
    "parameters": {
      "resource_group": {
        "defaultValue": "duffle-group",
        "type": "string"
      },
      "cluster_name": {
        "defaultValue": "my-aks-cluster",
        "type": "string"
      },
      "mysql_resource_group": {
        "defaultValue": "aks-wordpress-mysql",
        "type": "string"
      },
      "mysql_db_service_name": {
        "defaultValue": "mymysqldb",
        "type": "string"
      }
    },
    "credentials": {
      "azure_tokens": {
        "path": "/root/.azure/accessTokens.json"
      },
      "azure_profile": {
        "path": "/root/.azure/azureProfile.json"
      },
      "mysql_user": {
        "env": "MYSQL_USER"
      },
      "mysql_password": {
        "env": "MYSQL_PASSWORD"
      }
    }
}
